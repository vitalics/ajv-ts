{
  "name": "ajv-ts",
  "description": "JSON-schema builder with typescript safety",
  "version": "0.8.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "build": "tsx ./tsup.config.mts",
    "test": "vitest run",
    "test:watch": "vitest",
    "ci:version": "changeset version",
    "ci:publish": "changeset publish"
  },
  "author": "Vitali Haradkou <vitalicset@yandex.ru>",
  "homepage": "https://github.com/vitalics/ajv-ts",
  "keywords": [
    "ajv",
    "validation",
    "json-schema",
    "typescript",
    "builder"
  ],
  "bugs": {
    "url": "https://github.com/vitalics/ajv-ts/issues",
    "email": "vitalicset@yandex.ru"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.26.2",
    "@types/benchmark": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@vitest/ui": "1.6.0",
    "benchmark": "2.1.4",
    "eslint": "8.0.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-n": "15.0.0",
    "tsup": "8.1.0",
    "tsx": "4.17.0",
    "typescript": "5.5.3",
    "vitest": "1.6.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "ajv": "8.16.0",
    "ajv-errors": "3.0.0",
    "ajv-formats": "3.0.1",
    "type-fest": "4.26.0"
  }
}